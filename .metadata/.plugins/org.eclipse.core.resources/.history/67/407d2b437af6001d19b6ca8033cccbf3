package principal;

import entities.Conta;
import entities.ContaCorrente;

public class Program {

	public static void main(String[] args) {
		
		Conta cc = new ContaCorrente();

		cc.setTitularConta("Joao");
		cc.setNumeroConta(1234);
		cc.setAgencia(4321);
		cc.depositar(500.00);
		cc.mostraDadosDaConta();
		cc.sacar(483.33);
		cc.mostraDadosDaConta();
		
		
	}

}
